<ul class="track-list-container">
  <% for (const item of items) { %>
  <li class="track-list-item">
    <a style="
    font-family: 'Josefin Sans', sans-serif;
    font-weight: bold;
    font-size: 1rem;
  " class="track-list-title" href="<%- item.path %>">
      <%= item.title %>
      </a>
    <span class="track-list-desc"> <%= item.subtitle.length  > 256 ? item.subtitle.substring(0, 256) + "..." : item.subtitle %></span>
    <span class="track-list-icons">
      <img 
        src="assets/schedule-icon.svg" 
      />
      <span class="track-list-time"> 
        <%= "time" in item ? item.time: "5 minutes" %>
      </span> 
      <span class="<%- item.difficulty == 'Beginner' ? 'beginner': item.difficulty == 'Intermediate' ? 'intermediate' : 'advanced' %>"> <% for (const i in Array(3).fill(0)) { %> <span></span> <% } %> </span> <span class="track-list-diff"> <%= "difficulty" in item ? item.difficulty : "Beginner" %> </span>
    </span>
  </li>
  <% } %>
</ul>
